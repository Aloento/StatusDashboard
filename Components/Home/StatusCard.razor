<div class="flex h-fit flex-col overflow-hidden rounded-md bg-white px-3.5 py-5 shadow-md">
  <h3 class="px-3.5 text-2xl font-medium text-slate-800">
    Application Services
  </h3>

  <scale-divider class="h-auto" />

  <ul class="px-3.5">
    @for (var i = 0; i < Random.Shared.Next(1, 5); i++)
    {
      <ServiceItem Name="@(Enum.GetName((StatusType)i) + " Service")" Type="@(StatusType)i" />
    }
  </ul>
</div>
